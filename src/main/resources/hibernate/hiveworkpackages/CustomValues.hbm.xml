<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.raybiztech.hiveworkpackages.business.CustomValues"
		table="custom_values">
		<id name="id" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="customizedType" type="java.lang.String">
			<column name="customized_type"></column>
		</property>
		<property name="customizedId" type="java.lang.Long">
			<column name="customized_id"></column>
		</property>
		<property name="customFieldId" type="java.lang.Long">
			<column name="custom_field_id"></column>
		</property>
		<property name="value" type="java.lang.String">
			<column name="value"></column>
		</property>

	</class>

</hibernate-mapping>