<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.raybiztech.meetingrequest.business.Location"
		table="MeetingLocations">

		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>

		<property  name="locationName" type="java.lang.String">
			<column name="LocationName" />
		</property>
		
		<property  name="locationStatus" type="java.lang.Boolean">
			<column name="locationStatus" default="true" />
		</property>

	</class>

</hibernate-mapping>