<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.raybiztech.projectmanagement.invoice.business.SentInvoiceSummary"
		table="INVOICE_SUMMARY_SENTINVOICES">

		<id name="id" type="java.lang.Long">
			<column name="Id" />
			<generator class="native" />
		</id>

		<property name="sentInvoiceId" type="java.lang.Long">
			<column name="SentInvoiceId" />
		</property>

	</class>
</hibernate-mapping>